<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/propietarioFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Propietario.PropietarioFragment"
        android:label="PROPIETARIOS REGISTRADOS"
        tools:layout="@layout/fragment_propietario" />
    <fragment
        android:id="@+id/vehiculoFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Vehiculo.VehiculoFragment"
        android:label="VEHICULOS REGISTRADOS"
        tools:layout="@layout/fragment_vehiculo" />
    <fragment
        android:id="@+id/oficinaGobFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.OficinaGob.OficinaGobFragment"
        android:label="OFICINAS REGISTRADAS"
        tools:layout="@layout/fragment_oficina_gob" >
        <action
            android:id="@+id/action_oficinaGobFragment_to_agregarOficinaFragment"
            app:destination="@id/agregarOficinaFragment" />
        <action
            android:id="@+id/action_oficinaGobFragment_to_actualizarOficinaFragment"
            app:destination="@id/actualizarOficinaFragment" />
    </fragment>
    <fragment
        android:id="@+id/accidenteFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Accidente.AccidenteFragment"
        android:label="LISTA DE ACCIDENTES"
        tools:layout="@layout/fragment_accidente" >
        <action
            android:id="@+id/action_accidenteFragment_to_agregarAccidenteFragment"
            app:destination="@id/agregarAccidenteFragment" />
        <action
            android:id="@+id/action_accidenteFragment_to_actualizarAccidenteFragment"
            app:destination="@id/actualizarAccidenteFragment" />
    </fragment>
    <fragment
        android:id="@+id/infraccionFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Infraccion.InfraccionFragment"
        android:label="LISTA DE INFRACCIONES"
        tools:layout="@layout/fragment_infraccion" >
        <action
            android:id="@+id/action_infraccionFragment_to_infraccionRegisterFragment"
            app:destination="@id/infraccionRegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/audienciaFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Audiencia.AudienciaFragment"
        android:label="LISTA DE AUDIENCIA"
        tools:layout="@layout/fragment_audiencia" />
    <fragment
        android:id="@+id/normaDetFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.NormaDet.NormaDetFragment"
        android:label="NORMAS REGISTRADAS"
        tools:layout="@layout/fragment_norma_det" />
    <fragment
        android:id="@+id/agenteFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Agente.AgenteFragment"
        android:label="AGENTES REGISTRADOS"
        tools:layout="@layout/fragment_agente" />
    <fragment
        android:id="@+id/actaFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Acta.ActaFragment"
        android:label="ACTAS REGISTRADAS"
        tools:layout="@layout/fragment_acta" >
        <action
            android:id="@+id/action_actaFragment_to_actaRegisterFragment"
            app:destination="@id/actaRegisterFragment" />
    </fragment>
    <fragment
        android:id="@+id/zonaFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Zona.ZonaFragment"
        android:label="Zona"
        tools:layout="@layout/fragment_zona">
        <action
            android:id="@+id/action_zonaFragment_to_actualizarZonaFragment"
            app:destination="@id/actualizarZonaFragment" />
        <action
            android:id="@+id/action_zonaFragment_to_mapsZonaFragment"
            app:destination="@id/mapsZonaFragment" />
    </fragment>
    <fragment
        android:id="@+id/puestoControlFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.PuestoControl.PuestoControlFragment"
        android:label="Puesto de Control"
        tools:layout="@layout/fragment_puesto_control" >
        <action
            android:id="@+id/action_puestoControlFragment_to_registerPuestoControlFragment"
            app:destination="@id/registerPuestoControlFragment" />
    </fragment>


    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Login.LoginFragment"
        android:label="LOGIN"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Register.RegisterFragment"
        android:label="REGISTRO DE USUARIO"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/actualizarZonaFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Zona.ActualizarZonaFragment"
        android:label="ACTUALIZAR ZONA"
        tools:layout="@layout/fragment_actualizar_zona" />
    <fragment
        android:id="@+id/registerPuestoControlFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.PuestoControl.RegisterPuestoControlFragment"
        android:label="REGISTRO DE PUESTO DE CONTROL"
        tools:layout="@layout/fragment_register_puesto_control" />
    <fragment
        android:id="@+id/mapsZonaFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Zona.MapsZonaFragment"
        android:label="MAPA"
        tools:layout="@layout/fragment_maps_zona" />
    <fragment
        android:id="@+id/agregarAccidenteFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Accidente.AgregarAccidenteFragment"
        android:label="AGREGAR ACCIDENTE"
        tools:layout="@layout/fragment_agregar_accidente" />
    <fragment
        android:id="@+id/infraccionRegisterFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Infraccion.InfraccionRegisterFragment"
        android:label="AGREGAR INFRACCION"
        tools:layout="@layout/fragment_infraccion_register" />
    <fragment
        android:id="@+id/actualizarAccidenteFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Accidente.ActualizarAccidenteFragment"
        android:label="ACTUALIZAR ACCIDENTE"
        tools:layout="@layout/fragment_actualizar_accidente" />
    <fragment
        android:id="@+id/agregarOficinaFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.OficinaGob.AgregarOficinaFragment"
        android:label="AGREGAR OFICINA"
        tools:layout="@layout/fragment_agregar_oficina" />
    <fragment
        android:id="@+id/actualizarOficinaFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.OficinaGob.ActualizarOficinaFragment"
        android:label="ACTUALIZAR OFICINA"
        tools:layout="@layout/fragment_actualizar_oficina" />
    <fragment
        android:id="@+id/actaRegisterFragment"
        android:name="com.example.grupo_05_tarea_16_ejercicio_01.fragments.Acta.ActaRegisterFragment"
        android:label="REGISTRAR ACTA"
        tools:layout="@layout/fragment_acta_register" />
</navigation>